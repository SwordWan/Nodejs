<h3>
    <a href="{{{backurl}}}" class="actionBtn">返回管理员列表</a>
    <p>{{{title}}}</p>
</h3>
<form>
    <table width="100%" border="0" cellpadding="8" cellspacing="0" class="tableBasic">
		<tr>
			<td  width="90" align="right">roomid</td>
			<td align="right"><input type="text" name="roomid" id="roomid" value="{{row.roomid}}" size="50" class="inpMain" /></td>
		</tr>
		<tr>
			<td  width="90" align="right">roomuuid</td>
			<td align="right"><input type="text" name="roomuuid" id="roomuuid" value="{{row.roomuuid}}" size="50" class="inpMain" /></td>
		</tr>
		<tr>
			<td  width="90" align="right">creator</td>
			<td align="right"><input type="text" name="creator" id="creator" value="{{row.creator}}" size="50" class="inpMain" /></td>
		</tr>
		<tr>
			<td  width="90" align="right">clubid</td>
			<td align="right"><input type="text" name="clubid" id="clubid" value="{{row.clubid}}" size="50" class="inpMain" /></td>
		</tr>
		<tr>
			<td  width="90" align="right">gameid</td>
			<td align="right"><input type="text" name="gameid" id="gameid" value="{{row.gameid}}" size="50" class="inpMain" /></td>
		</tr>
		<tr>
			<td  width="90" align="right">roomargs</td>
			<td align="right"><input type="text" name="roomargs" id="roomargs" value="{{row.roomargs}}" size="50" class="inpMain" /></td>
		</tr>
		<tr>
			<td  width="90" align="right">ctime</td>
			<td align="right"><input type="text" name="ctime" id="ctime" value="{{row.ctime}}" size="50" class="inpMain" /></td>
		</tr>
		<tr>
			<td  width="90" align="right">tmlen</td>
			<td align="right"><input type="text" name="tmlen" id="tmlen" value="{{row.tmlen}}" size="50" class="inpMain" /></td>
		</tr>
		<tr>
			<td  width="90" align="right">playtimes</td>
			<td align="right"><input type="text" name="playtimes" id="playtimes" value="{{row.playtimes}}" size="50" class="inpMain" /></td>
		</tr>
		<tr>
			<td  width="90" align="right">allid</td>
			<td align="right"><input type="text" name="allid" id="allid" value="{{row.allid}}" size="50" class="inpMain" /></td>
		</tr>
		<tr>
			<td>&nbps;</td>
			<td align="right"><input name="submit" id="submit" class="btn" type="button" value="提交" /></td>
		</tr>
    </table>
</form>
<script>
    var edit = {{ edit }};
    var url = '{{posturl}}';
    $("#submit").click(function () {
        var e = false;
        var o = $("form").serializeJson();
        do {
            if (!edit) {

            }

            if (e) {
                break;
            }
            $.ajax({
                url: url,
                method: 'POST',
                dataType: 'json',
                data: o,
                error: function () { },
                success: function (response) {
                    $("#err_total").text(response.errmsg).show();
                }
            })
        } while (false);
    });
</script>